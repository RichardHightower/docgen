---
title: Package org.example.model Class Diagram
---

classDiagram
    class Manager{
        +Person manager
        +List<Person> employees
    }
    Manager -- Person: manager
    
    class Customer{
        +boolean elite
    }
    Customer -- Person: extends
    
    class Department {
        +Manager manager
    }
    Department -- Manager: manager
    
    class Address {
        +String street
        +String town
        +String country
        +String postalCode
    }
    
    class Person {
        +String firstName
        +String lastName
        +String workEmail
        +String email
        +Address address
    }
    Person <|-- Customer 
    Person <|-- Employee
    
    class Employee {
        +boolean newHire
    }
    Employee -- Person: extends
    
    Address <|-- Person: composition
